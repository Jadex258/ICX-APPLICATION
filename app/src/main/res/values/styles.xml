<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="TextInputLayoutAppearance" parent="Widget.Material3.AutoCompleteTextView.OutlinedBox">
        <item name="cornerSizeTopLeft">16dp</item>
        <item name="cornerSizeTopRight">16dp</item>
        <item name="cornerSizeBottomRight">16dp</item>
        <item name="cornerSizeBottomLeft">16dp</item>
        <item name="boxBackgroundColor">@color/corner_color</item>
    </style>


    <style name="PrivacyPolicyTextView">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:lineSpacingExtra">4dp</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:gravity">fill_horizontal</item>
        <item name="android:justificationMode">inter_word</item>
    </style>


    <style name="PrivacyPolicyLabelTextView">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">sans-serif-black</item>
    </style>


</resources>


